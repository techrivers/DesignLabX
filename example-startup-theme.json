  "colors": {
    "primary": "#ff6b35",
    "secondary": "#004e89",
    "error": "#e53e3e",
    "warning": "#dd6b20",
    "info": "#3182ce",
    "success": "#38a169",
    "background": {
      "default": "#ffffff",
      "paper": "#f7fafc"
    },
    "text": {
      "primary": "#1a202c",
      "secondary": "#4a5568"
    }
